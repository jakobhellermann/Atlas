<Wix xmlns="http://wixtoolset.org/schemas/v4/wxs">
    <Package Name="tbd" Manufacturer="JJ" Version="0.2.0.0"
        UpgradeCode="0d205f44-59e8-419f-a56b-11f7c9aa9211">
